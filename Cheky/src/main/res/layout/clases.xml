<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="600dp"
        android:text="Tus clases"
        android:textColor="#3F7D20"
        android:textSize="34sp"
        android:textStyle="bold"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/space"
        app:layout_constraintVertical_bias="0.186" />

    <Button
        android:id="@+id/button3"
        android:layout_width="363dp"
        android:layout_height="63dp"
        android:layout_marginTop="236dp"
        android:text="5IV7 Calculo Integral"
        android:textAlignment="textStart"
        android:backgroundTint="#67DC69"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="363dp"
        android:layout_height="63dp"
        android:layout_marginTop="296dp"
        android:text="5IV7 Física III"
        android:backgroundTint="#7DD5DD"
        android:textAlignment="textStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button6"
        android:layout_width="363dp"
        android:layout_height="63dp"
        android:layout_marginTop="356dp"
        android:text="5IV7 Química"
        android:textAlignment="textStart"
        android:backgroundTint="#B56CCF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button7"
        android:layout_width="363dp"
        android:layout_height="63dp"
        android:layout_marginTop="416dp"
        android:text="5IV7 Inglés"
        android:textAlignment="textStart"
        android:backgroundTint="#FB8E51"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="194dp"
        android:layout_marginEnd="11dp"
        android:clickable="true"
        app:backgroundTint="#3F7D20"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7"
        app:srcCompat="@android:drawable/ic_menu_add" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton2"
        android:layout_width="54dp"
        android:layout_height="60dp"
        android:clickable="true"
        app:backgroundTint="#3F7D20"
        app:srcCompat="@android:drawable/ic_popup_reminder"
        tools:layout_editor_absoluteX="290dp"
        tools:layout_editor_absoluteY="6dp" />

    <Button
        android:id="@+id/button8"
        android:layout_width="417dp"
        android:layout_height="72dp"
        app:backgroundTint="#3F7D20"
        tools:layout_editor_absoluteX="-3dp"
        tools:layout_editor_absoluteY="-6dp" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/floatingActionButton3"
        android:layout_width="71dp"
        android:layout_height="56dp"
        android:clickable="true"
        app:backgroundTint="#3F7D20"
        app:srcCompat="@android:drawable/ic_menu_myplaces"
        tools:layout_editor_absoluteX="344dp"
        tools:layout_editor_absoluteY="6dp" />

</androidx.constraintlayout.widget.ConstraintLayout>